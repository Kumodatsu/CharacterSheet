<Bindings>
    <Binding name="INCREMENT_HP" header="CHARACTER_SHEET">
        CS_API.Charsheet.increment_hp()
    </Binding>
    <Binding name="DECREMENT_HP">
        CS_API.Charsheet.decrement_hp()
    </Binding>
    <Binding name="TOGGLE_STATS_FRAME">
        CS_API.Interface.Toggle(CS_StatsFrame)
    </Binding>
    <Binding name="TOGGLE_EDIT_FRAME">
        CS_API.Interface.Toggle(CS_EditFrame)
    </Binding>
</Bindings>
