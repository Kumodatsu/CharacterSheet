<Bindings>
    <Binding name="INCREMENT_HP" header="CHARACTER_SHEET">
        CS_API.Charsheet.set_hp(CS_API.Charsheet.CurrentHP + 1)
    </Binding>
    <Binding name="DECREMENT_HP">
        CS_API.Charsheet.set_hp(CS_API.Charsheet.CurrentHP - 1)
    </Binding>
    <Binding name="TOGGLE_MAIN_FRAME">
        CS_API.Interface.ToggleMainFrame()
    </Binding>
    <Binding name="TOGGLE_STATS_FRAME">
        CS_API.Interface.ToggleStatsFrame()
    </Binding>
    <Binding name="TOGGLE_EDIT_FRAME">
        CS_API.Interface.ToggleEditFrame()
    </Binding>
</Bindings>
